{
  "manifest_version": 2,
  "name": "Miku Theme",
  "description": "A Hatsune Miku themed New Tab page for Firefox!",
  "version": "1.4",
  "author": "MalikHw47",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "https://raw.githubusercontent.com/*",
    "https://search.brave.com/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "miku-theme@malikhw47.com",
      "strict_min_version": "78.0"
    }
  }

}
