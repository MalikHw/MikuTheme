{
  "manifest_version": 3,
  "name": "Miku Theme",
  "description": "a Hatsune Miku themed New Tab page for chromium browsers!",
  "version": "1.8",
  "author": "MalikHw47",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://raw.githubusercontent.com/*",
    "https://duckduckgo.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "options_page": "settings.html"
}


